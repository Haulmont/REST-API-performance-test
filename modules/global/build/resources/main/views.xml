<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<views xmlns="http://schemas.haulmont.com/cuba/view.xsd">
    <view class="com.company.perftest.entity.Comment"
          name="comment-edit">
        <property name="request">
            <property name="number"/>
        </property>
        <property name="text"/>
        <property name="user">
            <property name="login"/>
            <property name="name"/>
        </property>
    </view>
    <view class="com.company.perftest.entity.Request"
          name="request-edit">
        <property name="number"/>
